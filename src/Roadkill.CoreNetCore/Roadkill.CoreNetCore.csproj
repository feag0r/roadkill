<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="Localization\readme.txt" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Database\Schema\MySql\Create.sql" />
